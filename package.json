{
  "name": "schoolmonitor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:en-US": "ng serve --configuration=en-US",
    "start:pa-Guru": "ng serve --configuration=pa-Guru",
    "build:en-US": "ng build --configuration=en-US",
    "build:pa-Guru": "ng build --configuration=pa-Guru",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --configuration=pa-Guru && ng build --configuration=en-US  && ng run schoolmonitor:server:production  &&  ng run schoolmonitor:server:en-US &&  ng run schoolmonitor:server:pa-Guru"
  },
